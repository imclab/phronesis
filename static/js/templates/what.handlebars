<script id="what_view" type="x-handlebars-template">
	<div id="instructions" class="hidden-top">
		<p><span id="three">What</span> type of person do you want to be?</p>
	</div>
	<div id="adjective_specifics"></div>
	<div id="adjective_container">
		{{#each adjectives}}
			<div class="adjective-wrap">
				<div id="{{ name }}" class="adjective">{{ name }}</div>
				<div class="more-info" style="background-color:white; display:none">More</div>
			</div>
		{{/each}}		
	</div>
</script>